   "source": [
    "import glob\n",
    "import os\n",
    "import PIL.Image\n",
    "\n",
    "\n",
    "\n",
    "filenames = sorted(glob.glob(\"*.tif\"))\n",
    "\n",
    "os.makedirs(\"png\", exist_ok=True)\n",
    "\n",
    "for fname in filenames:\n",
    "\n",
    "    im = PIL.Image.open(fname)\n",
    "    basename = os.path.basename(fname)\n",
    "    im.save(os.path.join(\"png\", basename.replace(\".tif\", \".png\")))"
   ]
